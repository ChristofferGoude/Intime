<div class="row">
<%= form_for(@project) do |f| %>	
	<div class="row">
		<div class="col-md-12">
			<div class="field form-group">
			    <%= f.label :projectname %><br>
			    <%= f.text_field :projectname, :class => "form-control" %>
		  	</div>
	  	</div>
  	</div>
	<div class="row">
		<div class="col-md-12">
		  	<div class="actions">
		    	<%= f.submit "Create Project", :class => "btn btn-default" %>
		  	</div>	  	
	  	</div>
	</div>
<% end %>
</div>
